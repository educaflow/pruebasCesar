<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

    <module name="configuracioncentro" package="com.educaflow.apps.configuracioncentro.db"/>
    <entity name="CursoAcademicoDepartamento">
        <many-to-one name="cursoAcademico" ref="com.educaflow.apps.configuracioncentro.db.CursoAcademico" required="true" title="NO"/>
        <many-to-one name="departamento" ref="com.educaflow.apps.configuracioncentro.db.Departamento" required="true"/>
        
        <unique-constraint columns="cursoAcademico,departamento" name="uc_curso_academico_departamento" title="NO"/>
    </entity>

</domain-models>