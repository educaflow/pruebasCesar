<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

    <module name="configuracioncentro" package="com.educaflow.apps.configuracioncentro.db"/>
    <entity name="CursoAcademico">
        <string name="code"  />
        <string name="name" namecolumn="true" />
        <many-to-one name="centro" ref="com.educaflow.apps.configuracioncentro.db.Centro" required="true"/>
        <one-to-many name="departamentos" ref="com.educaflow.apps.configuracioncentro.db.CursoAcademicoDepartamento" mappedBy="cursoAcademico"/>
        <one-to-many name="cargos" ref="com.educaflow.apps.configuracioncentro.db.CursoAcademicoCargo" mappedBy="cursoAcademico"/>
    </entity>

</domain-models>