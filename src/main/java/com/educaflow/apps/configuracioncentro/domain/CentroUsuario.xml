<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

    <module name="configuracioncentro" package="com.educaflow.apps.configuracioncentro.db"/>
    <entity name="CentroUsuario" repository="abstract">
        <many-to-one name="centro" ref="com.educaflow.apps.configuracioncentro.db.Centro" required="true"/>
        <many-to-one name="usuario" ref="com.axelor.auth.db.User" required="true"/>
        <enum name="tipoUsuario" ref="com.educaflow.apps.configuracioncentro.db.TipoUsuario" title="Tipo Usuario" required="true"/>

        <one-to-many
                name="cursoAcademicoCentroUsuarios"
                ref="com.educaflow.apps.configuracioncentro.db.CursoAcademicoCentroUsuario"
                mappedBy="centroUsuario"
                title="Cursos acadÃ©micos"
        />

        <unique-constraint columns="centro,usuario" name="uc_centro_usuario" title="NO"/>
    </entity>

</domain-models>