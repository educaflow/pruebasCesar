<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

    <module name="configuracioncentro" package="com.educaflow.apps.configuracioncentro.db"/>
    <entity name="Grupo" repository="abstract">
        <string name="code"  />
        <string name="name" namecolumn="true" />
        <enum name="tipoGrupo" ref="com.educaflow.apps.configuracioncentro.db.TipoGrupo" title="Tipo de Grupo"/>
        <many-to-one name="ciclo" ref="com.educaflow.apps.sistemaeducativo.db.Ciclo" title="Ciclo"/>
        <many-to-one name="centro" ref="com.educaflow.apps.configuracioncentro.db.Centro"/>
    </entity>


</domain-models>