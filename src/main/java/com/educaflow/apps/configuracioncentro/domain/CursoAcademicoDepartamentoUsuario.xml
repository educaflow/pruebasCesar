<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

    <module name="configuracioncentro" package="com.educaflow.apps.configuracioncentro.db"/>
    <entity name="CursoAcademicoDepartamentoUsuario">

        <many-to-one name="cursoAcademicoDepartamento" ref="com.educaflow.apps.configuracioncentro.db.CursoAcademicoDepartamento" required="true" title="NO"/>
        <many-to-one name="usuario" ref="com.axelor.auth.db.User" required="true"/>

        <unique-constraint columns="cursoAcademicoDepartamento,usuario" name="uc_curso_academico_departamento_cargo" title="NO"/>
    </entity>

</domain-models>