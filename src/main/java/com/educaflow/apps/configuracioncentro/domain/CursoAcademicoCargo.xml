<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

    <module name="configuracioncentro" package="com.educaflow.apps.configuracioncentro.db"/>
    <entity name="CursoAcademicoCargo">
        <many-to-one name="cursoAcademico" ref="com.educaflow.apps.configuracioncentro.db.CursoAcademico" required="true" title="NO"/>
        <many-to-one name="cargo" ref="com.educaflow.apps.configuracioncentro.db.Cargo" required="true" title="NO"/>

        <one-to-many
                name="cursoAcademicoCentroUsuarioCursoAcademicoCargos"
                ref="com.educaflow.apps.configuracioncentro.db.CursoAcademicoCentroUsuarioCursoAcademicoCargo"
                mappedBy="cursoAcademicoCargo"
                title="NO"/>

        <unique-constraint columns="cursoAcademico,cargo" name="uc_curso_academico_cargo" title="NO"/>
    </entity>

</domain-models>