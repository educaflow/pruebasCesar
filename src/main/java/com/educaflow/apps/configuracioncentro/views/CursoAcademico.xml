    <grid name="mant-CursoAcademico-CursoAcademicoGrupo-grid" title="" model="com.educaflow.apps.configuracioncentro.db.CursoAcademicoGrupo"
          groups="admins,users"
          editable="false" edit-icon="false" x-selector="none" canNew="true" canEdit="true" canDelete="true" canSave="true" canViewOnClick="true"
    >
        <field name="grupo" title="Grupo"/>
        <field name="curso" title="Curso"/>
        <field name="tutor.cursoAcademicoCentroUsuario.centroUsuario.usuario.name" title="Tutor"/>
        <field name="delegado.cursoAcademicoCentroUsuario.centroUsuario.usuario.name" title="Delegado"/>
        <field name="subdelegado.cursoAcademicoCentroUsuario.centroUsuario.usuario.name" title="Subdelegado"/>
    </grid>

    <form name="mant-CursoAcademico-CursoAcademicoGrupo-form"
          title="Curso Académico"
          model="com.educaflow.apps.configuracioncentro.db.CursoAcademicoGrupo"
          groups="admins,users"
          width="large" canAttach="false" canBack="false" canBackOnSave="false" canDelete="false" canNew="false"
    >
        <panel name="CursoAcademicoGrupo" title="Grupo">
            <field name="grupo" title="Grupo" readonlyIf="id != null"/>
            <field name="tutor"
                   title="Tutor"
                   target-name="cursoAcademicoCentroUsuario.centroUsuario.usuario.name"
                   domain="self.cursoAcademicoGrupo = :id AND self.cursoAcademicoCentroUsuario.centroUsuario.tipoUsuario = 'PROFESOR'"/>
            <field name="delegado"
                   title="Delegado"
                   target-name="cursoAcademicoCentroUsuario.centroUsuario.usuario.name"
                   domain="self.cursoAcademicoGrupo = :id AND self.cursoAcademicoCentroUsuario.centroUsuario.tipoUsuario = 'ALUMNO'"
            />
            <field name="subdelegado"
                   title="Subdelegado"
                   target-name="cursoAcademicoCentroUsuario.centroUsuario.usuario.name"
                     domain="self.cursoAcademicoGrupo = :id AND self.cursoAcademicoCentroUsuario.centroUsuario.tipoUsuario = 'ALUMNO'"
            />
        </panel>
        <panel-related
                field="cursoAcademicoCentroUsuarios"
                title="Usuarios"
                grid-view="mant-CursoAcademico-CursoAcademicoGrupo-CursoAcademicoCentroUsuarioCursoAcademicoGrupo-grid"
                form-view="view-CursoAcademicoCentroUsuario-form"
                canNew="false"
        />
    </form>

