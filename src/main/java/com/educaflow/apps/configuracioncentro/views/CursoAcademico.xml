    <grid name="mant-CursoAcademico-CursoAcademicoCentroUsuario-grid" title="" model="com.educaflow.apps.configuracioncentro.db.CursoAcademicoCentroUsuario"
          groups="admins,users"
          canViewOnClick="false" canDelete="true" canSave="true" editable="false" canNew="false">
        <field name="centroUsuario.usuario" title="Usuario"/>
        <field name="centroUsuario.tipoUsuario" title="Tipo de usuario"/>

    </grid>

    <form name="mant-CursoAcademico-CursoAcademicoCentroUsuario-form" title="Usuarios" model="com.educaflow.apps.configuracioncentro.db.CursoAcademicoCentroUsuario"
          groups="admins,users"
          width="large" canAttach="false" canBack="false" canBackOnSave="false" canDelete="true" canNew="true"
    >
        <panel title="Usuario">
            <field name="centroUsuario.usuario" readonly="true"/>
            <field name="centroUsuario.tipoUsuario" readonly="true"/>
        </panel>
        <panel-tabs >
            <panel-related
                    field="cursoAcademicoCentroUsuarioCursoAcademicoDepartamentos"
                    title="Departamentos"
                    grid-view="mant-CursoAcademico-CursoAcademicoCentroUsuario-CursoAcademicoCentroUsuarioCursoAcademicoDepartamento-grid"
                    form-view="mant-CursoAcademico-CursoAcademicoCentroUsuario-CursoAcademicoCentroUsuarioCursoAcademicoDepartamento-form"
                    showIf="centroUsuario.tipoUsuario != 'ALUMNO' &amp;&amp; centroUsuario.tipoUsuario != 'EXTERNO'"
            />
            <panel-related
                    field="cursoAcademicoCentroUsuarioCursoAcademicoCargos"
                    title="Cargos"
                    grid-view="mant-CursoAcademico-CursoAcademicoCentroUsuario-CursoAcademicoCentroUsuarioCursoAcademicoCargo-grid"
                    form-view="mant-CursoAcademico-CursoAcademicoCentroUsuario-CursoAcademicoCentroUsuarioCursoAcademicoCargo-form"
                    showIf="centroUsuario.tipoUsuario != 'EXTERNO'"
            />
            <panel-related
                    field="cursoAcademicoCentroUsuarioCursoAcademicoGrupos"
                    title="Grupos"
                    grid-view="mant-CursoAcademico-CursoAcademicoCentroUsuario-CursoAcademicoCentroUsuarioCursoAcademicoGrupo-grid"
                    form-view="mant-CursoAcademico-CursoAcademicoCentroUsuario-CursoAcademicoCentroUsuarioCursoAcademicoGrupo-form"
                    showIf="centroUsuario.tipoUsuario == 'ALUMNO' || centroUsuario.tipoUsuario == 'PROFESOR'"
            />
        </panel-tabs>
    </form>

