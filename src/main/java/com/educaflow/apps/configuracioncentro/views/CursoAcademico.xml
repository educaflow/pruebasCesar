    <!-- ********************** Vistas de CursoAcademico -> CursoAcademicoDepartamento ************************* -->
    <!-- ******************************************************************************************************* -->

    <grid name="mant-CursoAcademico-CursoAcademicoDepartamento-grid" title="" model="com.educaflow.apps.configuracioncentro.db.CursoAcademicoDepartamento"
          groups="admins,users"
          editable="false" edit-icon="false" x-selector="none" canNew="true" canEdit="true" canDelete="true" canSave="true" canViewOnClick="false">
        <field name="departamento" title="Departamento"/>
        <field name="jefeDepartamento.cursoAcademicoCentroUsuario.centroUsuario.usuario.name" title="Jefe departamento" />
    </grid>

    <form name="mant-CursoAcademico-CursoAcademicoDepartamento-form"
          title="Curso Académico"
          model="com.educaflow.apps.configuracioncentro.db.CursoAcademicoDepartamento"
          groups="admins,users"
          width="large" canAttach="false" canBack="false" canBackOnSave="false" canDelete="false" canNew="false"
          onNew="mant-CursoAcademico-CursoAcademicoDepartamento-onNew-action-record"
    >
        <panel name="CursoAcademicoDepartamento" title="Departamento">
            <field name="departamento" title="Departamento" readonlyIf="id"/>
            <field
                    name="jefeDepartamento"
                    title="Jefe Departamento"
                    target-name="cursoAcademicoCentroUsuario.centroUsuario.usuario.name"
                    domain="self.cursoAcademicoDepartamento = :id"
            />
        </panel>
        <panel-related
                field="cursoAcademicoCentroUsuarios"
                title="Profesores del departamento"
                grid-view="mant-CursoAcademico-CursoAcademicoDepartamento-CursoAcademicoCentroUsuarioCursoAcademicoDepartamento-grid"
                form-view="mant-CursoAcademico-CursoAcademicoDepartamento-CursoAcademicoCentroUsuarioCursoAcademicoDepartamento-form"
                canNew="false" canRemove="false" canEdit="false"
        />
    </form>

    <action-record
