              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.4.xsd">

    <action-view name="mant-CursoAcademico-action" title="Cursos Académicos" model="com.educaflow.apps.configuracioncentro.db.CursoAcademico">
        <view type="grid" name="mant-CursoAcademico-grid" />
        <view type="form" name="mant-CursoAcademico-form" />
        <view-param name="forceEdit" value="true" />
    </action-view>

    <grid name="mant-CursoAcademico-grid"
          title="Curso Académico"
          model="com.educaflow.apps.configuracioncentro.db.CursoAcademico"
          groups="admins,users"
          editable="false" edit-icon="false" x-selector="none" canNew="true" canEdit="false" canDelete="false" canSave="false" canViewOnClick="true"
    >
        <field name="name" title="Curso Académico" />
    </grid>

    <form name="mant-CursoAcademico-form" title="Curso Académico" model="com.educaflow.apps.configuracioncentro.db.CursoAcademico"
          groups="admins,users"
          width="large" canAttach="false" canBack="false" canBackOnSave="true" canDelete="true" canNew="false">
        <panel name="CursoAcademico" title="Curso académico">
            <field name="name" colOffset="9" />
        </panel>
        <panel-tabs>
            <panel-related
                    field="cursoAcademicoDepartamentos"
                    title="Departamentos"
                    grid-view="mant-CursoAcademico-CursoAcademicoDepartamento-grid"
                    form-view="mant-CursoAcademico-CursoAcademicoDepartamento-form"
            />
            <panel-related
                    field="cursoAcademicoCargos"
                    title="Cargos"
                    grid-view="mant-CursoAcademico-CursoAcademicoCargo-grid"
                    form-view="mant-CursoAcademico-CursoAcademicoCargo-form"
            />
            <panel-related
                    field="cursoAcademicoGrupos"
                    title="Grupos"
                    grid-view="mant-CursoAcademico-CursoAcademicoGrupo-grid"
                    form-view="mant-CursoAcademico-CursoAcademicoGrupo-form"
            />
            <panel-related
                    field="cursoAcademicoCentroUsuarios"
                    title="Usuarios"
                    grid-view="mant-CursoAcademico-CursoAcademicoCentroUsuario-grid"
                    form-view="mant-CursoAcademico-CursoAcademicoCentroUsuario-form"
                    canNew="false"
            />
        </panel-tabs>
    </form>

    <grid name="search-CursoAcademico-grid" title="Curso Académico" model="com.educaflow.apps.configuracioncentro.db.CursoAcademico"
          groups="admins,users"
          orderBy="name"
          editable="false" edit-icon="false" x-selector="none" canNew="false" canEdit="false" canDelete="false" canSave="false" canViewOnClick="true">
        <field name="name" />
    </grid>

